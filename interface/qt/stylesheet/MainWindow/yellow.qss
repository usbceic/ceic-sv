/***********************************
* Global
***********************************/

/*Efecto al seleccionar texto*/
* {
    selection-color: #212121;
    selection-background-color: #FFF9C4;
}

/***********************************
* Banner
***********************************/

/*Configurar el color de fondo de la ventana*/
#centralwidget{
     background: #FFEB3B;
}

/*Configurar el titulo del banner*/
#MainTitle {
    color: #FFFFFF;
    font-size: 22pt;
    font-weight: 500;
    text-transform: capitalize;
    qproperty-alignment: 'AlignVCenter | AlignLeft';
}

/*Configurar estilo por defecto del boton de nombre de usuario*/
#userBn0 {
    background: transparent;
    border: 1px solid transparent;
    border-radius: 2px;
    min-width: 6em;
    min-height: 2em;
    padding: 2px;
    color: #FFFFFF;
}

/*Configurar estilo del boton de nombre de usuario cuando el mouse pasa por encima de ellas y cuando son presionadas por el mismo*/
#userBn0:pressed, #userBn0::hover {
    background: #FBC02D;
}

/***********************************
* MenuBar
***********************************/

#MenuBar {
    background: #FFFFFF;
    border: none;
    border-top: 1px inset #BDBDBD;
    padding: -6px;
    min-width: 5em;
 }

#home, #sales, #inventory, #querys, #books, #loans, #clients, #users, #configure, #help{

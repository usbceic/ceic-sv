/*Configurar el estilo por defecto de los botones para agregar y quitar*/
#add0, #substract0{
    border: 1px solid transparent;
    border-radius: 2px;
    min-width: 2em;
    padding: 2px;
}

/*Configurar el estilo de los botones para agregar y quitar cuando se pasa por encima de ellos o son presionados*/
#add0:hover, #add0:pressed, #substract0:hover, #substract0:pressed{
    background: #FFF9C4;
}

/*Configuración del estilo por defecto del line edit*/
#spinLine0{
    padding: 2px;
    text-align: center;
    border: 1px solid transparent;
    border-right-color: #BDBDBD;
    border-left-color: #BDBDBD;
}

/*Configuración del estilo del line edit en modo de solo lectura*/
#spinLine0[readOnly="true"]{
    border-style: inset;
    border-color: silver;
    background: #FFF9C4;
}

/*Connfiguración del estilo por defecto del QFrame*/
#spinFrame0{
    background: #FFFFFF;
    border: 1px solid #BDBDBD;
    border-radius: 2px;
    padding: -9px;
}